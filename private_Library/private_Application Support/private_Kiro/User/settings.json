{
    "editor.minimap.enabled": false,
    "editor.autoClosingBrackets": "never",
    "editor.autoClosingQuotes": "never",
    "editor.cursorBlinking": "smooth",
    "editor.fontFamily": "HackGen, Menlo, Monaco, 'Courier New', monospace",
    "editor.fontSize": 14,
    "editor.lineHeight": 18,
    "editor.renderWhitespace": "boundary",
    "editor.wordWrap": "on",
    "files.autoGuessEncoding": true,
    "git.autofetch": true,
    "vim.hlsearch": true,
    "vim.leader": " ",
    "vim.normalModeKeyBindings": [
        {
            "before": [
                "j"
            ],
            "after": [
                "g",
                "j"
            ]
        },
        {
            "before": [
                "k"
            ],
            "after": [
                "g",
                "k"
            ]
        },
        {
            "before": [
                "q"
            ],
            "after": []
        },
        {
            "before": [
                "leader",
                "q"
            ],
            "after": [
                "q"
            ]
        },
        {
            "before": [
                ";"
            ],
            "after": [
                ":"
            ]
        },
        {
            "before": [
                "<C-n>"
            ],
            "after": [],
            "commands": [
                {
                    "command": ":nohl",
                    "args": []
                }
            ]
        },
        {
            "before": [
                "H"
            ],
            "after": [
                "^"
            ]
        },
        {
            "before": [
                "L"
            ],
            "after": [
                "$"
            ]
        }
    ],
    "files.insertFinalNewline": true,
    "editor.unicodeHighlight.allowedLocales": {
        "ja": true
    },
    "editor.unicodeHighlight.includeStrings": "inUntrustedWorkspace",
    "kiroAgent.enableTabAutocomplete": true,
    "terminal.external.osxExec": "Ghostty.app",
    "terminal.integrated.fontFamily": "HackGen",
    "terminal.integrated.fontSize": 14,
    "terminal.integrated.lineHeight": 1.2,
    "git.confirmSync": false,
    "kiroAgent.agentAutonomy": "Autopilot",
    "kiroAgent.trustedCommands": [
        "cat *",
        "git check-ignore *",
        "git diff *",
        "git log *",
        "git remote -v",
        "git show *",
        "git status *",
        "ls *",
        "npx zenn preview",
        "podman logs *",
        "podman-compose ps",
        "uv --version",
        "uv sync",
        "which *"
    ],
    "files.associations": {
        "*.json.j2": "json",
        "*.yaml.j2": "yaml"
    },
    "kiroAgent.modelSelection": "auto",
    "git.openRepositoryInParentFolders": "never",
    "telemetry.dataSharingAndPromptLogging.usageAnalyticsAndPerformanceMetrics": true,
    "telemetry.dataSharingAndPromptLogging.contentCollectionForServiceImprovement": true,
    "telemetry.dataSharingAndPromptLogging.promptLogging": false,
    "kiroAgent.codeReferences.referenceTracker": true,
    "kiroAgent.configureMCP": "Enabled",
    "viceroy.config.lastVersion": "0.53.202469",
    "files.watcherExclude": {
        "**/build/**": true,
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/.hg/store/**": true
    },
    "files.exclude": {
        ".viceroy": true,
        "apollo-overrides": true,
        "release-info": true,
        "src/*/build": false,
        "build": false,
        "env": true,
        "packageInfo*": true,
        ".bemol": true,
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true
    },
    "search.exclude": {
        "env": true,
        "src/*/build": true,
        "build": true,
        "**/node_modules": true,
        "**/bower_components": true,
        "**/*.code-search": true
    },
    "[brazilPackageConfig]": {
        "workbench.editor.languageDetection": false
    },
    "viceroy.config.internal.lastUpdateTime": "2025-11-19T05:43:05.770Z",
    "workbench.colorTheme": "One Dark Pro"
}
